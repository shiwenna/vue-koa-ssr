  "version": "0.2.0",
  "configurations": [


    
    {
      "type": "node",
      "request": "launch",
      "env": {
        "NODE_ENV": "development"
      },
      "name": "nodemon",
      "runtimeExecutable": "nodemon",
      "program": "${workspaceFolder}/server-entry.js",
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      // "debugStdLib": true
    }
  ]
}